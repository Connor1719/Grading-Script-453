/* boolean expressions involving multiple && and || operators */

int main(int x, int y) {
    int z;
    
    if ( x > y ) x = 0;
    if (x > 0 && x < 10 && y != 2) x = 1;
    if (x == 0 || x != -1 || z > 2) x = 2;

    while ( x > y ) x = 0;
    while (x > 0 && x < 10 && y != 2) x = 1;
    while (x == 0 || x != -1 || z > 2) x = 2;
}
