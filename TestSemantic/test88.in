/* if statements with expressions containing operators */

int x(int y) {
    y = 0;
}

int main() {
    int x, y, z, w;
    if (x > 0) x = 1;
    if (x + y * z / w > x - w / 2 * z) x = 1;
    if ( (x + y) * z / w > ((x - w) / 2) * z ) x = 1;
    if ( ( (x + y) * z / w) > ((x - w) / 2) * z ) x = 1;
}
