/* if statements with expressions containing function calls */

int f(int x) {
    x = 0;
}

int g(int x, int y) {
    x = y;
}

int main() {
    int x, y, z, w;
    if (f(x) > 0) x = 1;
    if (f(x + y * z / w) > g(x - w / 2 * z, 0)) x = 1;
    if ((x + y) * f(z / w) > g((x - w) / 2, 1) * z) x = 1;
    if (f((x + y) * z) > g((x - w) / 2, 2) * z) x = 1;
}
